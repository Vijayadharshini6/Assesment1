package asses.expre;
import java.util.Scanner;
public class expre{
public static void main(String[] args)
{
  Scanner sc=new Scanner(System.in);
  System.out.println("Input");
  String inp=sc.nextLine();
  int c=0;
  c=inp;
if(c>=0||c<0)
{
  System.out.println("Valid");
}
else
{
  System.out.println(Invalid");
}
}
}
